import random
players = []
# store the roles in a dictionary
roles = {'werewolf' : 0, 'villager' : 0}
players_info = {}

for i in roles('werewolf'):
    player = random.choice(players)
    while player in players_info:
        player = random.choice(players)
    players_info{player : 'werewolf'}

for j in roles('villager'):
    for player in players:
        if player not in players_info:
            players_info(player : 'villager')
 


